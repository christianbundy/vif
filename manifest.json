{
  "name": "VIF",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Because GIF sucks.",
  "homepage_url": "http://github.com/ChristianBundy/vif",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "http://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}